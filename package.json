{
  "name": "temp-vite-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate:items": "node scripts/generate-item-descriptions.cjs",
    "generate:species": "node scripts/generate-species-data.cjs",
    "generate:species-info": "node scripts/generate-species-info.cjs",
    "generate:moves": "node scripts/generate-moves.cjs",
    "generate:abilities": "node scripts/generate-abilities.cjs",
    "generate:scripts": "node scripts/generate-scripts.cjs",
    "generate:new-game-flags": "node scripts/generate-new-game-flags.cjs",
    "generate:flags": "node scripts/generate-flag-var-maps.cjs",
    "generate:all": "npm run generate:items && npm run generate:species && npm run generate:species-info && npm run generate:moves && npm run generate:abilities && npm run generate:scripts && npm run generate:new-game-flags && npm run generate:flags",
    "save:audit-layouts": "node --experimental-strip-types scripts/audit-save-layouts.ts"
  },
  "dependencies": {
    "@types/three": "^0.181.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "three": "^0.181.2",
    "upng-js": "^2.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/upng-js": "^2.1.5",
    "@vitejs/plugin-react": "^5.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "knip": "^5.71.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.2.2"
  }
}
