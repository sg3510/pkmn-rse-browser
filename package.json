{
  "name": "temp-vite-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate:items": "node scripts/generate-item-descriptions.cjs",
    "generate:species": "node scripts/generate-species-data.cjs",
    "generate:species-info": "node scripts/generate-species-info.cjs",
    "generate:moves": "node scripts/generate-moves.cjs",
    "generate:abilities": "node scripts/generate-abilities.cjs",
    "generate:scripts": "node scripts/generate-scripts.cjs",
    "generate:trainers": "node scripts/generate-trainer-ids.cjs",
    "generate:large-objects": "node scripts/generate-large-object-graphics.cjs",
    "generate:new-game-flags": "node scripts/generate-new-game-flags.cjs",
    "generate:flags": "node scripts/generate-flag-var-maps.cjs",
    "generate:metatile-labels": "node scripts/generate-metatile-labels.cjs",
    "generate:multichoice": "node scripts/generate-multichoice.cjs",
    "generate:field-effects": "node scripts/generate-field-effects.cjs",
    "generate:weather": "node scripts/generate-weather.cjs",
    "generate:weather-runtime": "node scripts/generate-weather-runtime.cjs",
    "generate:type-chart": "node scripts/generate-type-effectiveness.cjs",
    "generate:battle-moves": "node scripts/generate-battle-moves.cjs",
    "generate:trainer-parties": "node scripts/generate-trainer-parties.cjs",
    "generate:battle-backgrounds": "node scripts/generate-battle-backgrounds.cjs",
    "generate:battle-scripts": "node scripts/generate-battle-scripts.cjs",
    "generate:battle-animations": "node scripts/generate-battle-animations.cjs",
    "generate:battle-constants": "node scripts/generate-battle-constants.cjs",
    "generate:sprite-coords": "node scripts/generate-pokemon-sprite-coords.cjs",
    "validate:battle-sprites": "node scripts/validate-battle-sprites.cjs",
    "generate:learnsets": "node scripts/generate-learnsets.cjs",
    "generate:item-effects": "node scripts/generate-item-battle-effects.cjs",
    "perf:analyze:logs": "node scripts/perf/analyze-console-log.mjs",
    "perf:analyze:heapprofile": "node scripts/perf/analyze-heapprofile.mjs",
    "perf:analyze:heapsnapshot": "node scripts/perf/analyze-heapsnapshot.mjs",
    "perf:analyze:series": "node scripts/perf/compare-heapsnapshot-series.mjs --perf-dir docs/backlog/perf",
    "perf:analyze:all": "node scripts/perf/run-artifact-forensics.mjs --perf-dir docs/backlog/perf --reports-dir docs/backlog/perf/reports",
    "perf:test": "node --test scripts/perf/__tests__/*.test.mjs",
    "generate:all": "npm run generate:items && npm run generate:species && npm run generate:species-info && npm run generate:moves && npm run generate:abilities && npm run generate:scripts && npm run generate:trainers && npm run generate:large-objects && npm run generate:new-game-flags && npm run generate:flags && npm run generate:metatile-labels && npm run generate:multichoice && npm run generate:field-effects && npm run generate:weather && npm run generate:weather-runtime && npm run generate:type-chart && npm run generate:battle-moves && npm run generate:trainer-parties && npm run generate:battle-backgrounds && npm run generate:battle-scripts && npm run generate:battle-animations && npm run generate:battle-constants && npm run generate:sprite-coords && npm run generate:learnsets && npm run generate:item-effects",
    "verify:legendary-script-ingestion": "node scripts/verify-legendary-script-ingestion.cjs",
    "save:audit-layouts": "node --experimental-strip-types scripts/audit-save-layouts.ts"
  },
  "dependencies": {
    "@types/three": "^0.181.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "three": "^0.181.2",
    "upng-js": "^2.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/upng-js": "^2.1.5",
    "@vitejs/plugin-react": "^5.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "knip": "^5.71.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.2.2"
  }
}
